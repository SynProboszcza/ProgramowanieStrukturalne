#include <stdio.h>
#include <stdlib.h>

int main()
{
    printf("Podaj liczbe, ale naturalna:\n");
    int wybor = 0;
    int wynik = 0;
    scanf("%d", &wybor);
    if(wybor > 0){
	    for(int i=0; i<wybor; i++){
	    	wynik += i*i;
	    	//printf("%d\n",i);
	    //printf("%d\n",wynik);
	    }
	    printf("%d\n",wynik);
	}
	else {
		printf("Nie podales liczby naturalnej(wiekszej od zera)!");
	}
    return 0;
}
