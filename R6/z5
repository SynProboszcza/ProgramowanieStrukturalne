#include <stdio.h>
#include <stdlib.h>

int main()
{
    int min = 0;
    int max = 0;
    printf("Podaj min:");
    scanf("%d", &min);
    printf("Podaj max:");
    scanf("%d", &max);

    for(int i = min; i<max; i++){
        printf("%d\t%d\t%d",i,i*i,i*i*i);
        printf("\n");
    }
    return 0;
}
