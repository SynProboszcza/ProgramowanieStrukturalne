#include <stdio.h>
#include <stdlib.h>

int main()
{
    for(int i = 0; i<5; i++){
            switch(i){
            case 0:
                printf("$\n");
            break;
            case 1:
                printf("$$\n");
            break;
            case 2:
                printf("$$$\n");
            break;
            case 3:
                printf("$$$$\n");
            break;
            case 4:
                printf("$$$$$\n");
            break;

            }
        }
        printf("\n");

    return 0;
}
