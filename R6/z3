#include <stdio.h>
#include <stdlib.h>

int main()
{
    for(int i = 0; i<6; i++){
            switch(i){
            case 0:
                printf("F\n");
            break;
            case 1:
                printf("FE\n");
            break;
            case 2:
                printf("FED\n");
            break;
            case 3:
                printf("FEDC\n");
            break;
            case 4:
                printf("FEDCB\n");
            break;
            case 5:
                printf("FEDCBA\n");
            break;

            }
        }
        printf("\n");

    return 0;
}
